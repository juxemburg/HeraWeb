@model float

@{
    var value = Math.Min(1, Model) * 100;
    if (value <= 10)
    {
        <div class="progress progress-line-danger">
            <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" 
                 style="width: @value%;">
                <span class="sr-only">60% Complete</span>
            </div>
        </div>

    }
    if (value > 10 && value <= 25)
    {
        <div class="progress progress-line-warning">
            <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="90"
                 aria-valuemin="0" aria-valuemax="100" style="width: @value%;">

            </div>
        </div>
    }
    if (value > 25)
    {
        <div class="progress progress-line-success">
            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="90"
                 aria-valuemin="0" aria-valuemax="100" style="width: @value%;">
            </div>
        </div>
    }

}

