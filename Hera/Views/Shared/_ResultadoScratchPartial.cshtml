@model ResultadoScratch


<div class="card margin-10">
    <div class="header header-warning padding-10"
         style="height: 100%;">
        <h3>@Model.Nombre</h3>
    </div>
    <div class="content">
        <div>
            <dl>
                <dt>Scripts Muertos:</dt>
                <dd>@Model.DeadCodeCount</dd>

                <dt>Scripts Duplicados:</dt>
                <dd>@Model.DuplicateScriptsCount</dd>

                <dt>Total Bloques:</dt>
                <dd>@Model.NumBloques</dd>

                <dt>Total Scripts:</dt>
                <dd>@Model.NumScripts</dd>
            </dl>
        </div>
        <button type="button" class="btn btn-info" data-toggle="modal" data-target="#modal-@Model.Nombre">
            Bloques Usados
        </button>
    </div>
</div>


