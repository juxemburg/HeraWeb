@model PaginationViewModel<NotificationDateViewModel>
@{
    ViewData["Title"] = "Index";
}

<section class="section-thin">
    <div class="container-fluid">
        <h2><span class="title">Tus</span> Notificaciones</h2>
    </div>
</section>
@if (Model.Count > 0)
{
    <section class="section section-gray">
        <div class="container">
            <div class="row">

                @foreach (var item in Model)
                {
                    <div class="col-sm-6 col-md-4 col-lg-3">
                        <div class="card card-list card-raised">
                            <div class="card-content">
                                <h6 class="category text-info">Resumen</h6>
                                <h3 class="card-title text-center">@item.Resumed</h3>
                                @Html.Partial("_NotificationListPartial",
                               item.Notifications)
                            </div>
                        </div>
                    </div>

                }
            </div>
        </div>
    </section>
}

